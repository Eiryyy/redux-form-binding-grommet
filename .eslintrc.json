{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "never"
    ],
    "no-var": [
      "error"
    ],
    "eqeqeq": [
      "error"
    ],
    "no-useless-computed-key": [
      "error"
    ],
    "no-duplicate-imports": [
      "error"
    ],
    "prefer-const": [
      "error", {
        "destructuring": "any",
        "ignoreReadBeforeAssign": false
      }
    ],
    "arrow-spacing": [
      "error", {
        "before": true,
        "after": true
      }
    ],
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "jsx-quotes": [
      "error",
      "prefer-double"
    ],
    "no-unused-vars": [
      "error", {
        "ignoreRestSiblings": true
      }
    ]
  }
}
